{
	"usesTypeScript": true,
	"usesCssModule": true,
	"cssPreprocessor": "css",
	"testLibrary": "None",
	"component": {
		"default": {
			"customTemplates": {
				"index": "templates/component/index.ts",
				"component": "templates/component/TemplateName.tsx",
				"style": "templates/component/TemplateName.module.css"
			},
			"path": "src/components",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false,
			"withIndex": true
		},
		"page": {
			"customTemplates": {
				"component": "templates/page/TemplateNamePage.tsx",
				"style": "templates/page/TemplateNamePage.module.css",
				"index": "templates/page/index.ts"
			},
			"path": "src/pages",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false,
			"withIndex": true
		},
		"context": {
			"customTemplates": {
				"component": "templates/context/context.ts",
				"provider": "templates/context/provider.tsx",
				"types": "templates/context/types.ts",
				"index": "templates/context/index.ts"
			},
			"path": "src/contexts",
			"withStyle": false,
			"withTest": false,
			"withStory": false,
			"withLazy": false,
			"withIndex": true,
			"withProvider": true,
			"withTypes": true
		}
	},
	"hook": {
		"customTemplates": {
			"component": "templates/hook/useTemplateName.ts"
		},
		"path": "src/hooks",
		"withStyle": false,
		"withTest": false,
		"withStory": false,
		"withLazy": false
	}
}
